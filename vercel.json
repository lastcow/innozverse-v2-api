{
  "buildCommand": "cd apps/api && npm run build",
  "outputDirectory": "apps/api/dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev --workspace=@repo/api",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/apps/api/dist/index.js"
    }
  ]
}
